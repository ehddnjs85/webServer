            <div>
                <h1>INDEX</h1>
                <h5 style="float: right;" role="button" class="btn btn-primary" id="logout">logout</h5>
            </div>
            <div>
                <button type="button" class="btn btn-primary" onclick="location.href='main/user'">users</button>
                <button type="button" class="btn btn-primary">devices</button>
                <button type="button" class="btn btn-primary">setting</button>
