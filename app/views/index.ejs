<script>
$(document).ready(function() {
    if (!document.cookie.includes('user')) {
        window.location.href="/";
    }
    
    setInterval(function () {
        if (!document.cookie.includes('uuid')) {
            
            window.location.href="/";
        }
    }, 60 * 10000);
});
    
$('#logout').click(function () {
    window.location.href='/logout';
})
</script>